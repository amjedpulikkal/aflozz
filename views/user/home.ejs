      <script>

        function addtocart(id) {
          document.getElementById(`but-${id}`).classList.add("hidden")
          document.getElementById(`spinner-${id}`).classList.remove("hidden")
          console.log("goooo");
          console.log(id);
          fetch(`/cart/${id}`, {
            method: "POST"
          })
            .then(res => {
              console.log(res);
              if (res.ok) {

                res.json().then(data => {
                  toaster(1, "Success! a dd to cart.")
                  document.getElementById(`but-${id}`).classList.remove("hidden")
                  document.getElementById(`spinner-${id}`).classList.add("hidden")
                  document.getElementById("cartCount")?.classList.remove("hidden")
                  document.getElementById("cartN-1").innerHTML = data.cartLength
                })
              } else {
                document.getElementById(`but-${id}`).classList.remove("hidden")
                document.getElementById(`spinner-${id}`).classList.add("hidden")
                toaster(2, "Sorry,product is out of stock.")

                console.log(res.status);
              }
            }).catch(err => {
              console.log(err);
            })
        }

      </script>
