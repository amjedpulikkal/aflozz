      document.getElementById("logout").addEventListener("submit", async (e) => {
        e.preventDefault()

        const registration = await navigator.serviceWorker.register('/service-worker.js', { scope: "/" })
        let subscribe = await registration.pushManager.getSubscription()
        if (subscribe) {
          await subscribe.unsubscribe();
          fetch("/unsubscribe", { method: "POST", headers: { 'Content-Type': 'application/json' }, body: JSON.stringify(subscribe) }).then(async res => {
            if (res.ok) {
              console.log("ooooook");
              document.getElementById("logout").submit()
            }
          })
        } else {
          
          document.getElementById("logout").submit()

        }

        return true

      })
