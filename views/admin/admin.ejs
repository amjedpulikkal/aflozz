<script>
  // line chart data
  var buyerData = {
     labels :["January", "February", "March", "April", "May", "June","July", "August", "September", "October", "November", "December"],
    datasets: [
      {
        fillColor: "rgba(172,194,132,0.4)",
        strokeColor: "#ACC26D",
        pointColor: "#fff",
        pointStrokeColor: "#9DB86D",
        data: [ "<%=chart[chart.length-12]?.totalProductsSold||0%>", "<%=chart[chart.length-11]?.totalProductsSold||0%>", "<%=chart[chart.length-10]?.totalProductsSold||0%>", "<%=chart[chart.length-9]?.totalProductsSold||0%>", "<%=chart[chart.length-8]?.totalProductsSold||0%>", "<%=chart[chart.length-7]?.totalProductsSold||0%>","<%=chart[chart.length-6]?.totalProductsSold||0%>", "<%=chart[chart.length-5]?.totalProductsSold%>", "<%=chart[chart.length-4]?.totalProductsSold%>", "<%=chart[chart.length-3]?.totalProductsSold%>", "<%=chart[chart.length-2]?.totalProductsSold%>", "<%=chart[chart.length-1]?.totalProductsSold%>"]
      }
    ]
  }
  // get line chart canvas
  var buyers = document.getElementById('buyers').getContext('2d');
  // draw line chart
  new Chart(buyers).Line(buyerData);
  // pie chart data
  var pieData = [
    {
      value: 20,
      color: "#878BB6"
    },
    {
      value: 40,
      color: "#4ACAB4"
    },
    {
      value: 10,
      color: "#FF8153"
    },
    {
      value: 30,
      color: "#FFEA88"
    }
  ];
  // pie chart options
  var pieOptions = {
    segmentShowStroke: false,
    animateScale: true
  }
  // get pie chart canvas
  var countries = document.getElementById("countries").getContext("2d");
  // draw pie chart
  new Chart(countries).Pie(pieData, pieOptions);
  // bar chart data
  var barData = {
    labels: ["January", "February", "March", "April", "May", "June"],
    datasets: [
      {
        fillColor: "#48A497",
        strokeColor: "#48A4D1",
        data: [456, 479, 324, 569, 702, 600]
      },
      {
        fillColor: "rgba(73,188,170,0.4)",
        strokeColor: "rgba(72,174,209,0.4)",
        data: [364, 504, 605, 400, 345, 320]
      }
    ]
  }
  // get bar chart canvas
  var income = document.getElementById("income").getContext("2d");
  // draw bar chart
  new Chart(income).Bar(barData);
</script>